{
    "$schema": "http://json-schema.org/draft-04/schema",
    "title": "flowNode/set",
    "type": "object",
    "description": "Set definition.",
    "allOf": [ { "$ref": "glTFProperty.schema.json" } ],
    "properties": {
        "pointer": {
            "type": "string",
            "description": "JSON pointer of the property to write to."
        },
        "variableNode": {
            "allOf": [ { "$ref": "glTFid.schema.json" } ],
            "description": "The index of the variable node the written values are taken from."
        },        
        "name": { },
        "extensions": { },
        "extras": { }
    },
    "required": [
        "pointer",
        "variableNode"
    ]
}
