{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "oneOf": [
        {
            "title": "Variable Reference",
            "description": "Reference to a variable defined in the same behavior by using the index of the variable in the behavior's variale array",
            "type": "object",
            "properties": {
                "$variable": {
                    "type": "integer"
                }
            },
            "required": ["$variable"]
        },
        {
            "title": "Node Return Value Reference",
            "description": "Reference to the returned value of another node.",
            "type": "object",
            "properties": {
                "$node": {
                    "type": "integer"
                },
                "socket": {
                    "description": "Identifier ",
                    "type": "string"
                }
            },
            "required": ["$node"]
        }
    ]
}