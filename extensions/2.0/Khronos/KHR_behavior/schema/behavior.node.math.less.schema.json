{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "title": "Less Comparison Node",
    "type": "object",
    "allOf": [ { "$ref": "glTFProperty.schema.json" } ],
    "properties": {
        "type": {
            "const": "math/less"
        },
        "flow": {
            "type": "object",
            "properties": {
                "next": { "$ref": "behavior.node.flow.reference.schema.json" }
            }
        },
        "parameters": {
            "type": "object",
            "properties": {
                "condition": { "$ref": "behavior.types.boolean.schema.json" }
            },
            "required": ["condition"]
        }
    },
    "required": ["type", "parameters"]
}
