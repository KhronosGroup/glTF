            "minItems" : 1,
            "maxItems" : 1,
            "default" : [1029],
            "gltf_detailedDescription" : "Integer value passed to `cullFace()`. Valid values correspond to WebGL enums: `1028` (FRONT), `1029` (BACK), and `1032` (FRONT_AND_BACK).",
            "gltf_webgl" : "`cullFace()`"
        },
        "depthFunc" : {
            "type" : "array",
            "description" : "Integer values passed to depthFunc().",
