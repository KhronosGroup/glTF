{
    "$schema" : "http://json-schema.org/draft-03/schema",
    "title" : "functions",
    "type" : "object",
    "description" : "Arguments for fixed-function rendering state functions other than `enable()`/`disable()`.",
    "extends" : { "$ref" : "glTFProperty.schema.json" },
    "properties" : {
        "blendColor" : {
            "type" : "array",
