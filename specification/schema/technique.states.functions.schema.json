            "gltf_webgl" : "`depthFunc()`"
        },
        "depthMask" : {
            "type" : "array",
            "description" : "Boolean value passed to `depthMask()`.",
            "items" : {
                "type" : "boolean"
            },
            "minItems" : 1,
            "maxItems" : 1,
            "default" : [true],
